#Arithmetic Operators
def add(a,b):
    return a+b
def sub(a,b):
    return a-b
def mul(a,b):
    return a*b
def div(a,b):
    return a/b
def mod(a,b):
    return a//b
def rem(a,b):
    return a%b
a=int(input())
b=int(input())
print(add(a,b))
print(sub(a,b))
print(mul(a,b))
print(div(a,b))
print(mod(a,b))
print(rem(a,b))

#Sqaure Root
def sqrt(n):
    return n**2
n=int(input())
print(sqrt(n))

#Cube Root
def cubrt(n):
    return n**3
n=int(input())
print(cubrt(n))
